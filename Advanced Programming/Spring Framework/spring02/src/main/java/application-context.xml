<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="l1" class="com.kn.bean.Laptop">
		<property name="sNo" value="111"></property>
		<property name="manufacturer" value="hp"></property>
		<property name="cost" value="989876"></property>
	</bean>
	<bean id="l2" class="com.kn.bean.Laptop">
		<property name="sNo" value="112"></property>
		<property name="manufacturer" value="Dell"></property>
		<property name="cost" value="100986"></property>
	</bean>
	<bean id="l3" class="com.kn.bean.Laptop">
		<constructor-arg name="sNo" value="113"/>
		<constructor-arg name="manufacturer" value="Lenovo"/>
		<constructor-arg name="cost" value="120986"/>
	</bean>
</beans>